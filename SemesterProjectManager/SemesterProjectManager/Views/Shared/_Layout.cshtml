                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="contact.html">Contact</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" asp-area="" asp-controller="Admin" asp-action="Index">Users</a>
                                    </li>
                                </ul>
                            </div>
                        </nav> <!-- nav -->
